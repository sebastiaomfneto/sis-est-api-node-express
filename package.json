    "url": "git+https://github.com/smfnto/sis-est-api-node-express.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smfnto/sis-est-api-node-express/issues"
  },
  "homepage": "https://github.com/smfnto/sis-est-api-node-express#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "sequelize": "^5.21.3",
    "sqlite3": "^4.1.1",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.2",
